# Copy this file to .env.local and fill in your values
# Never commit .env.local with real keys!

# ============================================
# REQUIRED - Core Services
# ============================================

# Supabase (Database & Auth)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_URL=https://your-project.supabase.co

# Supabase Storage Buckets
SUPABASE_BUCKET_PUBLIC=public-bucket-name
SUPABASE_ASSETS_BUCKET=assets-bucket-name

# Replicate API (AI Models - REQUIRED for video generation)
REPLICATE_API_TOKEN=r8_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# OpenAI API (GPT-4 for assistant)
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Cloudflare R2 (File Storage)
R2_ACCOUNT_ID=your-cf-account-id
R2_ACCESS_KEY_ID=your-r2-access-key-id
R2_SECRET_ACCESS_KEY=your-r2-secret-access-key
R2_BUCKET=your-bucket-name
R2_PUBLIC_BASE_URL=https://your-bucket.r2.dev
R2_S3_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
R2_UPLOAD_CACHE_CONTROL=public, max-age=31536000

# ============================================
# OPTIONAL - Additional Features
# ============================================

# Stripe (Payments & Billing)
STRIPE_SECRET_KEY=sk_test_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_PRICE_BASIC=price_xxxxxxxxxxxxx
NEXT_PUBLIC_PRICE_PRO=price_xxxxxxxxxxxxx

# ElevenLabs (Text-to-Speech)
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Sieve API (Legacy - may not be needed)
SIEVE_API_KEY=sk-your-sieve-api-key-here

# Sync Labs API (Lip sync)
SYNC_API_KEY=your-sync-api-key
SYNC_PROXY_API_KEY=your-sync-proxy-key
NEXT_PUBLIC_SYNC_API_KEY=your-sync-api-key

# Wavespeed API (Audio processing)
WAVESPEED_API_KEY=your-wavespeed-api-key

# Tavily API (Web search)
TAVILY_API_KEY=tvly-xxxxxxxxxxxxxxxxxxxxxxxxxx

# TikHub API (TikTok scraping)
TIKHUB_API_BASE=https://api.tikhub.io
TIKHUB_API_KEY=your-tikhub-api-key

# Meta/Facebook Ads API
META_ACCESS_TOKEN=your-meta-access-token
META_ADS_ACCESS_TOKEN=your-meta-ads-access-token
FACEBOOK_ADS_ACCESS_TOKEN=your-facebook-ads-access-token

# Apify (Web scraping)
APIFY_TOKEN=apify_api_xxxxxxxxxxxxxxxxxxxxxx

# Cloudflare KV (Task state management)
CLOUDFLARE_ACCOUNT_ID=your-cf-account-id
CLOUDFLARE_API_TOKEN=your-cf-api-token
CLOUDFLARE_KV_NAMESPACE_ID_TASKS=your-kv-namespace-id
CF_ACCOUNT_ID=your-cf-account-id
CF_API_TOKEN=your-cf-api-token
CF_KV_NAMESPACE_ID_TASKS=your-kv-namespace-id

# Browser Worker (Remote browser automation)
BROWSER_WORKER_URL=https://your-browser-worker.com
BROWSER_WORKER_SECRET=your-browser-worker-secret

# ============================================
# Application Configuration
# ============================================

# App URLs
NEXT_PUBLIC_BASE_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000

# Node Environment
NODE_ENV=development

# Python Path (for Python scripts)
PYTHON_PATH=/usr/bin/python3

# ============================================
# Replicate Model Versions (Optional overrides)
# ============================================

REPLICATE_IMAGE_MODEL=google/nano-banana
REPLICATE_EMBED_MODEL=replicate/all-mpnet-base-v2
REPLICATE_ESRGAN_MODEL=nightmareai/real-esrgan
REPLICATE_ESRGAN_VERSION=your-version-id
REPLICATE_LATENTSYNC_MODEL=zsxkib/latentsync
REPLICATE_LATENTSYNC_VERSION=your-version-id
REPLICATE_PLANNER_MODEL=meta/meta-llama-3-70b-instruct
REPLICATE_SEGMENT_MODEL=meta/meta-llama-3-8b-instruct
REPLICATE_SEGMENT_IDEAS_MODEL=meta/meta-llama-3-8b-instruct
REPLICATE_WAN_I2V_MODEL=alibaba-pai/wan

# ============================================
# Vercel Blob (Legacy - if using Vercel Blob instead of R2)
# ============================================

# BLOB_READ_WRITE_TOKEN=vercel_blob_rw_XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
